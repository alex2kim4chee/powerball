{
  "name": "powerball-ru",
  "private": true,
  "version": "0.1.0",
  "description": "PowerBall RU — неаффилированная лендинговая страница на Next.js",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build:cf": "next-on-pages",
    "create:cf": "wrangler pages project create powerball-pool",
    "deploy:cf": "wrangler pages deploy .vercel/output/static --project-name powerball-pool",
    "deploy:cf:prod": "wrangler pages deploy .vercel/output/static --project-name powerball-pool --branch main"
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "qrcode": "^1.5.3",
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "typescript": "5.9.2",
    "@cloudflare/next-on-pages": "^1.11.0",
    "wrangler": "^3.68.0"
  }
}
